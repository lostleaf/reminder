.center.hero-unit
  %h1= msg
  %img{width: "300", height:"300", src: "img/#{action}.jpg"}
  %br
  %a.btn.btn-large.btn-primary{href: got_url} Got It
  %button.btn.btn-large{onclick: "audioPause()"} Be Quiet
  %br
  %audio{autoplay: 'autoplay'}
    %source{src: "music/#{action}.ogg",type: "audio/ogg"}
    %source{src: "music/#{action}.mp3",type: "audio/mpeg"}
  :javascript
    var audio=document.getElementsByTagName("audio")[0]
    function audioPause(){
    	audio.pause();
    }
